{
    "name": "eat-well-app",
    "version": 2,
    "alias": ["eat-well-app.now.sh"],
    "builds": [
      { "src": "client/package.json", 
        "use": "@now/static-build",
        "config": { "distDir": "build" } 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ],
    "github": {
      "enabled": true
    }
  }
  